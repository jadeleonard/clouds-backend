generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model faq {
  id          String @id @default(auto()) @map("_id") @db.ObjectId
  description String
  title       String
}

model hero {
  id          String @id @default(auto()) @map("_id") @db.ObjectId
  description String
  title       String
}

model navbar {
  id   String @id @default(auto()) @map("_id") @db.ObjectId
  href String
  name String
}

model user {
  id       String @id @default(auto()) @map("_id") @db.ObjectId
  address  String
  email    String
  lastname String
  name     String
  password String
}
